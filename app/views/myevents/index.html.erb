<h1>Myevents</h1>

<div id="myevents">
  <% @myevents.each do |myevent| %>
    <%= render myevent %>
    
    <br>
    <%= link_to "Show this myevent", myevent, class:"bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded-full m-8" %>
  <% end %>
</div>
<br>
<%= link_to "New myevent", new_myevent_path, class:"bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded-full m-8" %>
